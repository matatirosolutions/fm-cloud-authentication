{
  "name": "matatirosoln/fm-cloud-authentication",
  "description": "Authenticate against FileMaker cloud for Data API or Admin API access",
  "type": "library",
  "authors": [
    {
      "name": "Steve Winter",
      "email": "steve@msdev.nz"
    }
  ],
  "keywords": [
    "FileMaker",
    "Cognito",
    "Data API",
    "DAPI",
    "Admin API"
  ],
  "license": [
    "MIT"
  ],
  "autoload": {
    "psr-4": {
      "MSDev\\FMCloudAuthenticator\\": "src"
    }
