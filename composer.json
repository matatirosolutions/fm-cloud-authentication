  "require": {
    "php": "7.4.*||8.0.*||8.1.*||8.2.*",
    "aws/aws-sdk-php": "^3.198",
    "guzzlehttp/guzzle": "^7.3",
    "nesbot/carbon": "^2.53",
    "phpseclib/phpseclib": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
